    "@nestjs/websockets": "^10.4.4",
    "@types/multer": "^1.4.12",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",

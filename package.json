    "@types/express": "^4.17.17",
    "@types/html-to-text": "^9.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.10",
    "@types/nodemailer": "^6.4.16",
    "@types/passport-jwt": "^4.0.1",
